<div class="theme-back-to-pagetop-float{% if _ENV.mode == 'canvas' %} theme-back-to-pagetop-float--canvas-mode{% endif %}">
	<div class="theme-back-to-pagetop-float__inner">
		<a href="#pagetop" class="theme-back-to-pagetop-float__back-to-top">
			<span class="theme-back-to-pagetop-float__back-to-top-inner">
				<span class="theme-back-to-pagetop-float__back-to-top-decoration-a">{%
					if _ENV.lang == "ja" %}このページの先頭へ戻る{%
					else %}Back to page top{%
					endif %}</span>
			</span>
		</a>
	</div>
</div>
